

*** icc ***
seq:
  export OMP_NUM_THREADS=1; icc -fast adi.base.c -DORIO_REPS=1 -DT=512 -DN=512
par:
  export OMP_NUM_THREADS=4; icc -fast -parallel adi.base.c -DORIO_REPS=1 -DT=512 -DN=512

*** pluto ***
seq:
  export OMP_NUM_THREADS=1; icc -fast adi.pluto.seq.c -DORIO_REPS=1 -DT=512 -DN=512
par:
  export OMP_NUM_THREADS=4; icc -fast -openmp adi.pluto.par.c -DORIO_REPS=1 -DT=512 -DN=512

*** pluto+ancc ***
seq:
  export OMP_NUM_THREADS=1; icc -fast -openmp adi.pluto_ancc.seq_par.c -DORIO_REPS=1 -DT=512 -DN=512
par:
  export OMP_NUM_THREADS=4; icc -fast -openmp adi.pluto_ancc.seq_par.c -DORIO_REPS=1 -DT=512 -DN=512



